<?xml version="1.0" encoding="UTF-8"?>
<component name="org.skinsoft.nuxeo.ide.uml.demo.lifecycle.definition">
  <extension point="lifecycle" target="org.nuxeo.ecm.core.lifecycle.LifeCycleService">
    <lifecycle initial="state 1" lifecyclemanager="jcrlifecyclemanager" name="MyFirstLifecycle">
      <transitions>
        <transition destinationState="state 2" name="to state 2"/>
      </transitions>
      <states>
        <state name="state 1">
          <transitions>
            <transition>to state 2</transition>
          </transitions>
        </state>
        <state name="state 2"/>
      </states>
    </lifecycle>
  </extension>
</component>
